<table>
    <tr>
        <th>ISBNコード</th><th>書名</th><th>価格</th>
        <th>出版社</th><th>刊行日</th><th>DL</th>
    </tr>    
<% @books.each do |book| %> <%#これによってhello_controller.rbの@bookから持ってくる%>
    <tr>
        <td><%= book.isbn %></td>
        <td><%= book.title %></td>
        <td><%= book.price %>円</td>
        <td><%= book.publish %></td>
        <td><%= book.published %></td>
        <td><%= book.dl %></td>
    </tr>
<% end %>
</table>